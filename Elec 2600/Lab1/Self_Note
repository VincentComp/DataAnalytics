   "source": [
    "trials = range(50)\n",
    "\n",
    "# method 1\n",
    "Alice_rolls = []\n",
    "for i in trials:\n",
    "    Alice_rolls.append(Alice.roll())\n",
    "    \n",
    "# method 2\n",
    "Bob_rolls = [Bob.roll() for i in trials]\n",
    "\n",
    "\n",
    "# create plot\n",
    "fig, ax = plt.subplots()\n",
    "ax.plot(trials, Alice_rolls, 'o', label='Alice')\n",
    "ax.plot(trials, Bob_rolls, 'o', label='Bob')\n",
    "\n",
    "# label the plots\n",
    "plt.xlabel('trial')\n",
    "plt.ylabel('outcome')\n",
    "plt.grid()\n",
    "plt.legend(bbox_to_anchor = (1,1))\n",
    "plt.show()"
   ]
