    "class die: \n",
    "    def __init__(self, dots = [1, 2, 3, 4, 5, 6]):\n",
    "        self.dots = dots\n",
    "    \n",
    "    def roll(self):\n",
    "    \treturn random.choice(self.dots)\n",
    "\n",
    "Alice = die([2, 6, 7])\n",
    "Bob = die([1, 5, 9])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "b14ee145-9b0a-4c6f-8fd0-c0f1a07821c1",
